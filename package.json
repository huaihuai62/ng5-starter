{
  "name": "ng-start",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "mock": "json-server --watch src/app/mock/db.json",
    "start": "ng serve --port=4201 --open",
    "start-proxy": "ng serve --open --proxy-config proxy.conf.json",
    "start-proxy-multi": "ng serve --open --proxy-config proxy.conf.js",
    "mock-server": "concurrently \"yarn run mock\" \"yarn run start-proxy\"",
    "build": "ng build -prod --stats-json -aot -ec --extract-licenses=false",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.3",
    "@angular/common": "^5.1.3",
    "@angular/compiler": "^5.1.3",
    "@angular/core": "^5.1.3",
    "@angular/forms": "^5.1.3",
    "@angular/http": "^5.1.3",
    "@angular/platform-browser": "^5.1.3",
    "@angular/platform-browser-dynamic": "^5.1.3",
    "@angular/router": "^5.1.3",
    "angular-in-memory-web-api": "^0.5.2",
    "core-js": "^2.4.1",
    "normalize.css": "^7.0.0",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.3",
    "@angular/compiler-cli": "^5.1.3",
    "@angular/language-service": "^5.0.0",
    "@types/jasmine": "^2.8.2",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^8.0.53",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "json-server": "^0.12.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.14.9",
    "postcss-sass": "^0.2.0",
    "protractor": "^5.2.0",
    "stylelint": "^8.3.0",
    "stylelint-config-standard": "^18.0.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.0"
  }
}
